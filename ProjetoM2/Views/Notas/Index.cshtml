@model IEnumerable<ProjetoM2.Models.Notas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.M1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SUB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Previsao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Media)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.M1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.M2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SUB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Previsao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Media)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
